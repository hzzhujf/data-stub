var BaseModal = require('../modal')

var modal = {
  table: '<%= tableName %>',
  fields: {
<% columns.forEach(function(item){ %>
    '<%= item.field %>': {
      type: <%= item.targetType %>,
      name: '<%= item.field %>',
      comment: '<%= item.comment %>',
    },
<% }); %>
  }
}
module.exports = new BaseModal(modal)
